{
	"name": "apured/generar-formulario",
	"description": "Generador de Formularios basado en DB o Modelo",
	"type": "project",
	"require": {
		"php": ">=5.6.4",
		"laravelcollective/html": "^5.8",
		"doctrine/dbal": ">=2.9.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~5.0"
	},
	"autoload": {
		"psr-4": {
			"ApuRed\\GenerarFormulario\\": "src"
		}
	},
	"license": "MIT",
	"homepage": "https://apured.com",
    "support": {
        "issues": "https://github.com/ApuRed/GenerarFormulario/issues",
        "source": "https://github.com/ApuRed/GenerarFormulario"
    },
	"authors": [{
		"name": "FaredCesar",
		"email": "cesar@apured.com"
	}],
    "extra": {        
        "laravel": {
            "providers": [
                "ApuRed\\GenerarFormulario\\GenerarFormularioServiceProvider"
            ],
            "aliases": {
                "GenerarFormulario": "ApuRed\\GenerarFormulario\\Facades\\Formgenerator"
            }
        }
    },
	"minimum-stability": "dev"
}
